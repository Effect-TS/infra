{
    "name": "multus-cni-network",
    "type": "multus",
    "capabilities": {
        "portMappings": true
    },
    "delegates": [
        {
            "name":"kube-ovn",
            "cniVersion":"0.3.1",
            "plugins":[
                {
                    "type":"kube-ovn",
                    "server_socket":"/run/openvswitch/kube-ovn-daemon.sock"
                },
                {
                    "type":"portmap",
                    "capabilities":{
                        "portMappings":true
                    }
                }
            ]
        }
    ],
    "kubeconfig": "/etc/rancher/k3s/k3s.yaml"
}
